{
  // Editor Config
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.insertSpaces": true,
  "editor.lineNumbers": "relative",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.tabSize": 2,
  "editor.bracketPairColorization.enabled": true,
  "editor.detectIndentation": true,
  "editor.rulers": [
    80,
    100
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "window.title": "${project}${dirty}${activeEditorShort}${separator}${rootName}${separator}${appName}",
  "workbench.colorTheme": "Dracula Theme",
  "security.workspace.trust.untrustedFiles": "open",
  // Emmet Config
  "emmet.includeLanguages": {
    "erb": "html",
    "javascript": "javascriptreact"
  },
  // Javascript Config
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  // Python Config
  "python.pythonPath": "/usr/bin/python3",
  "jupyter.askForKernelRestart": false,
  "[python]": {
    "editor.formatOnType": true
  },
  // Ruby Config
  "rubyLsp.rubyVersionManager": {
    "versionManager": "asdf"
  },
  "rubyLsp.formatter": "auto",
  "rubyLsp.enabledFeatures": {
    "codeActions": true,
    "diagnostics": true,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": true,
    "hover": true,
    "inlayHint": true,
    "onTypeFormatting": true,
    "selectionRanges": true,
    "semanticHighlighting": true,
    "completion": true,
    "codeLens": true
  },
  "sorbet.enabled": true,
  "files.associations": {
    "*.html.erb": "erb"
  },
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "misogi.ruby-rubocop"
  },
  "solargraph.transport": "stdio",
  "solargraph.diagnostics": true,
  // Tailwind Config
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.includeLanguages": {
    "erb": "html"
  },
  "tailwindCSS.experimental.classRegex": [
    "\\bclass:\\s*'([^']*)'",
    "\\bclass:\\s*\"([^\"]*)\""
  ],
  // Vim Config
  "vim.handleKeys": {
    "<C-p>": false,
    "<C-d>": true,
    "<C-f>": false
  },
  "vim.leader": "<space>",
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "J"
      ],
      "after": [
        "10",
        "j"
      ]
    },
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "leader",
        "w"
      ],
      "commands": [
        ":w!"
      ]
    },
    {
      "before": [
        "leader",
        "q"
      ],
      "commands": [
        ":q!"
      ]
    },
    {
      "before": [
        "leader",
        "i"
      ],
      "commands": [
        "editor.action.revealDefinition"
      ]
    },
    {
      "before": [
        "leader",
        "h"
      ],
      "after": [
        "ctrl",
        "w",
        "h"
      ]
    },
    {
      "before": [
        "leader",
        "shift",
        "l"
      ],
      "commands": [
        "workbench.action.navigateRight"
      ]
    }
  ],
  // Atlassian Config
  "yaml.schemas": {
    "file:///home/renan/.vscode/extensions/atlassian.atlascode-3.0.10/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "atlascode.bitbucket.enabled": true,
  "atlascode.jira.enabled": false,
  "atlascode.bitbucket.issues.createJiraEnabled": true,
  // Others
  "redhat.telemetry.enabled": false,
  "thunder-client.codeSnippetLanguage": "curl",
  "todohighlight.isEnable": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "yaml": true
  },
  "terminal.integrated.fontFamily": "Fira Code",
  "gitlens.statusBar.enabled": false,
  "workbench.settings.applyToAllProfiles": [
    "terminal.integrated.fontFamily"
  ],
  "editor.formatOnSave": true,
  "workbench.sideBar.location": "right",
  "editor.unicodeHighlight.nonBasicASCII": false,
  "github.copilot.editor.enableAutoCompletions": true,
  "vim.useSystemClipboard": true,
  "i18n-ally.displayLanguage": "pt-BR",
}